package selfProgramming;

class MathClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		double x = 28;    
        double y = 4;    
          
        // return the maximum of two numbers  
        System.out.println("Maximum number of x and y is: " + Math.max(x, y));   
        
     // return the minimum of two numbers  
        System.out.println("Minimum number of x and y is: " + Math.min(x, y)); 
          
        // return the square root of y   
        System.out.println("Square root of y is: " + Math.sqrt(y));   
          
        //returns 28 power of 4 i.e. 28*28*28*28    
        System.out.println("Power of x and y is: " + Math.pow(x, y));      
  
        // return the logarithm of given value       
        System.out.printf("Logarithm of x is: %.2f" , Math.log(x));   
        System.out.printf("\nLogarithm of y is: %.2f" , Math.log(y));  
          
        // return the logarithm of given value when base is 10      
        System.out.printf("\nlog10 of x is: %.2f" , Math.log10(x));   
        System.out.printf("\nlog10 of y is: %.2f" , Math.log10(y));       
  
        // return a power of 2    
        System.out.printf("\nexp of a is: %.2f" , Math.exp(x));    

	}

}
